{
    "version": "0.2.0",
    "configurations": [
      {
        "name": ".NET Core Launch (webapi)",
        "type": "coreclr",
        "request": "launch",
        "preLaunchTask": "build-api",
        "program": "${workspaceFolder}/api_recomendation/bin/Debug/net7.0/api_recomendation.dll",
        "args": [],
        "cwd": "${workspaceFolder}/api_recomendation",
        "stopAtEntry": false,
        "serverReadyAction": {
          "action": "openExternally",
          "pattern": "\\bNow listening on:\\s+(https?://\\S+)"
        },
        "env": {
          "ASPNETCORE_ENVIRONMENT": "Development"
        }
      },
      {
        "name": "ng serve",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:4200",
        "webRoot": "${workspaceFolder}/recommendationfront",
        "cwd":"${workspaceFolder}/recommendationfront",
        "preLaunchTask": "serve",
      }
  
    ]
  }
  